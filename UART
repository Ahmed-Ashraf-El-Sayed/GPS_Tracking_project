#include "stdint.h"
#include "C:\Keil\ARM\ARMCC\include\tm4c123gh6pm.h"
#include <string.h>
void SystemInit(){};
void UART5_INIT(){
SYSCTL_RCGCUART_R |= 0x00000002;	//Activate UART1	
 SYSCTL_RCGCGPIO_R |= 0x00000004; //Activate port c
UART5_CTL_R &= ~0x00000001;
UART5_IBRD_R = 104;  
UART5_FBRD_R = 11;
// Insert BaudRate 
UART5_LCRH_R = 0x00000070  ;
UART5_CTL_R |= 0x00000001;
// enable UARTportc
UART1_CTL_R &= ~ 0x00000001;
UART1_IBRD_R = 104;       //INT(16,000,000/(169600))
UART1_FBRD_R = 11;        //ROUND (0.166666764)
UART1_LCRH_R &=  0x00000070;
UART1_CTL_R |=  0x00000301;
GPIO_PORTC_DEN_R |= 0x30;
GPIO_PORTC_AFSEL_R |= 0x30;
GPIO_PORTC_PCTL_R = (GPIO_PORTC_PCTL_R&0xFF00FFFF)+0x00220000;
GPIO_PORTC_AMSEL_R &= ~0x30;
}

unint8_t UART_InChar(void){
while((UART5_FR_R&0x0010) != 0);
return((unint8_t)(UART5_DR_R&0xFF));
}
nt main(){
 while (1) {
UART5_INIT();
UART_InChar();	 	 
}
}
